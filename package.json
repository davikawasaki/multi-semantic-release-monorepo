{
  "name": "multi-semantic-release-monorepo",
  "version": "0.0.0",
  "description": "Management release for monorepo using multi semantic releases",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "postinstall": "lerna bootstrap",
    "presemantic-release": "npm i",
    "semantic-release": "multi-semantic-release --sequential-init --debug --first-parent --no-ci"
